    <script>
        document.getElementById('GenDownloadExcel').addEventListener('click', function () {
            // Data array
            // var G_DefTempExcel = [
            //     { ID: 1, NameTitle: "Ser" },
            //     { ID: 2, NameTitle: "Name" },
            //     { ID: 3, NameTitle: "Age" }
            // ];

            let ReptModel = $('#SendModelExcel').val()
            var ListdataExcel = JSON.parse(ReptModel);

            // Create a new workbook
            var workbook = XLSX.utils.book_new();

            // Convert the data array to a worksheet
            var worksheet = XLSX.utils.json_to_sheet(ListdataExcel);

            // Append the worksheet to the workbook
            XLSX.utils.book_append_sheet(workbook, worksheet, "Sheet1");

            // Convert the workbook to a binary string
            var wbout = XLSX.write(workbook, { bookType: 'xlsx', type: 'binary' });

            // Function to trigger download
            function s2ab(s) {
                var buf = new ArrayBuffer(s.length);
                var view = new Uint8Array(buf);
                for (var i = 0; i < s.length; i++) view[i] = s.charCodeAt(i) & 0xFF;
                return buf;
            }

            // Create a Blob from the binary string and trigger download
            var blob = new Blob([s2ab(wbout)], { type: "application/octet-stream" });
            var link = document.createElement('a');
            link.href = URL.createObjectURL(blob);
            link.download = $("#NameFileSaveExcel").val() + ".xlsx";
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
        });
    </script>
    <script>


        function getDeviceType() {
            var userAgent = navigator.userAgent || navigator.vendor || window.opera;

            // Check for Android
            if (/android/i.test(userAgent)) {
                return "Android";
            }

            // Check for iPhone/iPad/iPod
            if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
                return "iOS";
            }

            // Check for Windows
            if (/windows phone/i.test(userAgent)) {
                return "Windows Phone";
            }

            if (/windows/i.test(userAgent)) {
                return "Windows";
            }

            // Check for MacOS
            if (/macintosh/i.test(userAgent)) {
                return "MacOS";
            }

            // Check for Linux
            if (/linux/i.test(userAgent)) {
                return "Linux";
            }

            // Default to unknown
            return "Unknown";
        }


        $('#TxtDeviceType').val(getDeviceType())


    </script>

