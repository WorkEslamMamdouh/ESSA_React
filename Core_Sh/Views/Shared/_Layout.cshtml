    <script>
        function generateQRCode() {
            const text = document.getElementById("numberInput").value;

            if (!text) {
                alert("Ÿäÿ±ÿ¨Ÿâ ÿ•ÿØÿÆÿßŸÑ ŸÜÿµ ŸÑÿ•ŸÜÿ¥ÿßÿ° ÿ±ŸÖÿ≤ QR!");
                return;
            }

            // ŸÖÿ≥ÿ≠ ÿ±ŸÖÿ≤ QR ÿßŸÑÿ≥ÿßÿ®ŸÇ
            const qrContainer = document.getElementById("qrcode");
            const IDUtf8Text = document.getElementById("IDUtf8Text");
            qrContainer.innerHTML = "";

            // ÿ™ÿ±ŸÖŸäÿ≤ ÿßŸÑŸÜÿµ ÿ®ÿßÿ≥ÿ™ÿÆÿØÿßŸÖ UTF-8 ŸÑÿØÿπŸÖ ÿßŸÑŸÑÿ∫ÿ© ÿßŸÑÿπÿ±ÿ®Ÿäÿ©
            const utf8Text = unescape(encodeURIComponent(text));
            IDUtf8Text.value = encodeURIComponent(text)
            // ÿ•ŸÜÿ¥ÿßÿ° ÿ±ŸÖÿ≤ QR ÿ®ÿßÿ≥ÿ™ÿÆÿØÿßŸÖ ŸÖŸÉÿ™ÿ®ÿ© qrcode-generator
            const qr = qrcode(0, 'L'); // ŸÖÿ≥ÿ™ŸàŸâ ÿ™ÿµÿ≠Ÿäÿ≠ ÿßŸÑÿÆÿ∑ÿ£ L
            qr.addData(utf8Text);
            qr.make();

            // ÿ•ÿØÿÆÿßŸÑ ÿßŸÑŸÉŸàÿØ ŸÅŸä ÿßŸÑÿπŸÜÿµÿ±
            qrContainer.innerHTML = qr.createImgTag(4); // ÿ≠ÿ¨ŸÖ ÿµÿ∫Ÿäÿ± ŸÖŸÜÿßÿ≥ÿ® ŸÑŸÑÿ∑ÿ®ÿßÿπÿ©

            // ÿ∂ÿ®ÿ∑ ÿπÿ±ÿ∂ Ÿàÿßÿ±ÿ™ŸÅÿßÿπ ÿßŸÑÿµŸàÿ±ÿ©
            const qrImage = qrContainer.querySelector('img'); // ÿ≠ÿØÿØ ÿπŸÜÿµÿ± <img>
            qrImage.style.width = "100px";
            qrImage.style.height = "100px";
        }
    </script>

