@using Core.UI.Models

<style> 
	.u-btn {
		margin-top: 0px !important;
		margin-bottom: 2px !important;
	}
</style>
<div class="sparkline8-graph col-xs-12" style="border-radius: 50px;">



	<form action="https://forms.nicepagesrv.com/v2/form/process" class="u-clearfix u-form-spacing-10 u-form-vertical u-inner-form" style="padding: 10px" source="email" name="form">
		<div class="u-form-email u-form-group u-form-partition-factor-9">
		</div>
		 
		<div class="u-form-group u-form-name u-form-partition-factor-6">
			<label for="drpPeriod" class="u-label u-text-grey-40">@Res.Period</label>
			<select id="drpPeriod" name="select" class="u-input u-input-rectangle">
			 
			</select>
		</div>


		<div class="u-form-group u-form-name u-form-partition-factor-6">
			<label for="" class="u-label u-text-grey-40">@Res.From_Date</label>
			<input type="date" placeholder="MM/DD/YYYY" id="Txt_FromTrData" disabled="disabled" name="date" class="u-input u-input-rectangle" required="" data-date-format="mm/dd/yyyy">
		</div>

		<div class="u-form-group u-form-name u-form-partition-factor-6">
			<label for="" class="u-label u-text-grey-40">@Res.To_Date</label>
			<input type="date" placeholder="MM/DD/YYYY" id="Txt_TOTrData" name="date" disabled="disabled" class="u-input u-input-rectangle" required="" data-date-format="mm/dd/yyyy">
		</div>
		
		<div class="u-form-group u-form-name u-form-partition-factor-6 ">
			<label for="" class="u-label u-text-grey-40"> </label>
			<a id="btnClose" style="width: 100%;background-color: #a50b0b;cursor:pointer;" class="Style_Add_Item u-btn u-btn-submit u-input u-input-rectangle disabledDiv">@Res.Close</a>
		</div>
		
		<div class="u-form-group u-form-name u-form-partition-factor-6">
			<label for="" class="u-label u-text-grey-40"> </label>
			<a id="btnprint" style="width: 100%;background-color: #ffab06;" class="Style_Add_Item u-btn u-btn-submit u-input u-input-rectangle">@Res.Print_Tax_Report</a>
		</div>

		 
		<div class="u-form-email u-form-group u-form-partition-factor-9" style="text-align: -webkit-center;">
			<h6 id="btnDelete_Filter" class="display_none" style="width: 20%; cursor: pointer;">‚ùå</h6>
		</div>
	</form>

	<div class="inside-table my-4"> 
		<label for="" style="font-weight: 900;" class="u-label ">@Res.Sales</label>
		<br>
		<br>
		<div id="_Grid">
		</div> 
	</div>
	<form action="https://forms.nicepagesrv.com/v2/form/process" class="u-clearfix u-form-spacing-10 u-form-vertical u-inner-form" style="padding: 10px" source="email" name="form">
		<div class="u-form-group u-form-name u-form-partition-factor-3">
		</div>

		<div class="u-form-group u-form-name u-form-partition-factor-8">
			<label for="TotalSales" class="u-label u-text-grey-40">@Res.Net_Sales</label>
			<label id="TotalSales" style="font-weight: bold;" class="u-label "></label>
		</div>

		<div class="u-form-group u-form-name u-form-partition-factor-8">
		</div>

		<div class="u-form-group u-form-name u-form-partition-factor-8">
			<label for="TotalSalesRet" class="u-label u-text-grey-40">@Res.Net_Return</label>
			<label id="TotalSalesRet" style="font-weight: bold;" class="u-label  "></label>
		</div>
		<div class="u-form-group u-form-name u-form-partition-factor-8">
		</div>
		<div class="u-form-group u-form-name u-form-partition-factor-6">
			<label for="TotalSalesVat" class="u-label u-text-grey-40">@Res.Net_Tax</label>
			<label id="TotalSalesVat" style="font-weight: bold;" class="u-label   "></label>
		</div>
	</form>
	
	<div class="inside-table my-4"> 
		<label for="" style="font-weight: 900;" class="u-label ">@Res.Purchases</label>
		<br>
		<br>
		<div id="_Grid1">
		</div> 
	</div>
	<form action="https://forms.nicepagesrv.com/v2/form/process" class="u-clearfix u-form-spacing-10 u-form-vertical u-inner-form" style="padding: 10px" source="email" name="form">
		<div class="u-form-group u-form-name u-form-partition-factor-3">
		</div>

		<div class="u-form-group u-form-name u-form-partition-factor-8">
			<label for="TotalPur" class="u-label u-text-grey-40">@Res.Net_Purchases</label>
			<label id="TotalPur" style="font-weight: bold;" class="u-label "></label>
		</div>

		<div class="u-form-group u-form-name u-form-partition-factor-8">
		</div>

		<div class="u-form-group u-form-name u-form-partition-factor-8">
			<label for="TotalPurRet" class="u-label u-text-grey-40">@Res.Net_Return</label>
			<label id="TotalPurRet" style="font-weight: bold;" class="u-label  "></label>
		</div>
		<div class="u-form-group u-form-name u-form-partition-factor-8">
		</div>
		<div class="u-form-group u-form-name u-form-partition-factor-6">
			<label for="TotalPurVat" class="u-label u-text-grey-40">@Res.Net_Tax</label>
			<label id="TotalPurVat" style="font-weight: bold;" class="u-label "></label>
		</div>
	</form>
	<form action="https://forms.nicepagesrv.com/v2/form/process" class="u-clearfix u-form-spacing-10 u-form-vertical u-inner-form" style="padding: 10px" source="email" name="form">
 
	<div class="u-form-group u-form-name u-form-partition-factor-6">
		<label for="DueVat" class="u-label u-text-grey-40">@Res.Tax_due</label>
			<input type="text" placeholder="0" id="DueTax" disabled="disabled" name="DueTax" class="u-input u-input-rectangle" required=""> 
	</div>
	</form>

</div>


  
<script src="~/ClientApp/TaxReport/TaxReport.js"></script> 
