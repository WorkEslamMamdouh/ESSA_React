@using Core.UI.Models


<style>

    .u-btn {
        margin-top: 0px !important;
        margin-bottom: 2px !important;
    }
</style>
<div class="sparkline8-graph col-xs-12" style="border-radius: 50px;">



    <form action="https://forms.nicepagesrv.com/v2/form/process" class="u-clearfix u-form-spacing-10 u-form-vertical u-inner-form" style="padding: 10px" source="email" name="form">
        <div class="u-form-email u-form-group u-form-partition-factor-9">
        </div>

        <div class="u-form-email u-form-group u-form-partition-factor-7">
            <label for="" class="u-label u-text-grey-40">@Res.From_Date</label>
            <input type="date" placeholder="0" id="FromDate" class=" u-input u-input-rectangle">
        </div>
        <div class="u-form-group u-form-name u-form-partition-factor-7">
            <label for="" class="u-label u-text-grey-40">@Res.To_Date</label>
            <input type="date" placeholder="0" id="ToDate" class=" u-input u-input-rectangle">
        </div>


        <div class="u-form-group u-form-name u-form-partition-factor-7">
            <label for="drpCatID" class="u-label u-text-grey-40">@Res.Category</label>
            <select id="drpCatID" name="select" class="u-input u-input-rectangle">
            </select>
        </div>

        <div class="u-form-group u-form-name u-form-partition-factor-7">
            <label for="drpFamilyID" class="u-label u-text-grey-40">@Res.Item_Family</label>
            <select id="drpFamilyID" name="select" class="u-input u-input-rectangle">
                <option value="null">@Res.All</option>
            </select>
        </div> 
        <div class="u-form-group u-form-name u-form-partition-factor-7">
            <label for="" class="u-label u-text-grey-40"> </label>
            <input type="button" id="btnItem" style="width: 100%; border-radius:10px 10px 10px 10px ;  background-color: rgb(103 169 204)" class="Style_Add_Item u-btn u-btn-submit u-input u-input-rectangle" value="@Res.Item">
            <input type="hidden" placeholder="0" id="ItemID" class=""> 
        </div>
        <div class="u-form-group u-form-name u-form-partition-factor-9">
            <label for="" class="u-label u-text-grey-40">@Res.IncludeOpeningBalance</label>
            <input type="checkbox" checked placeholder="0" id="IncludeOpeningBalance" class=" u-input u-input-rectangle" style="width: 35px;height: 35px;margin-left: 100px;">
        </div> 
        <div class="u-form-group u-form-name u-form-partition-factor-7">
           
        </div>
        <div class="u-form-group u-form-name u-form-partition-factor-7">
            <label for="drpType" class="u-label u-text-grey-40">@Res.Report_Type</label>
            <select id="drpType" name="select" class="u-input u-input-rectangle">
                <option value="0">حركة مخزون ملخص</option>
                <option value="1">حركة مخزون تفصيلي</option>
                <option value="2">تكاليف مخزون </option>
            </select>
        </div>



        <div class="u-form-group u-form-name u-form-partition-factor-9"></div>
        <div class="u-form-email u-form-group u-form-partition-factor-9">

            <h6 id="btnDelete_Filter" class=" " style="cursor: pointer;">❌</h6>
        </div>

    </form>
    <form action="https://forms.nicepagesrv.com/v2/form/process" class="u-clearfix u-form-spacing-10 u-form-vertical u-inner-form" style="padding: 10px" source="email" name="form">
        <div class="u-form-email u-form-group u-form-partition-factor-5"></div>

        <div class="u-form-group u-form-name u-form-partition-factor-5">
            <a id="PrintPdf" style="width: 100%; border-radius:10px 10px 10px 10px ;cursor: pointer; background-color: #830202;" class="Style_Add_Item u-btn u-btn-submit u-input u-input-rectangle">
                @Res.Print_Pdf  <i class="fa fa-file-pdf-o"></i>
            </a>
        </div>


        <div class="u-form-group u-form-name u-form-partition-factor-5">
            <a id="PrintExcel" style="width: 100%; border-radius:10px 10px 10px 10px ;cursor: pointer; " class="Style_Add_Item u-btn u-btn-submit u-input u-input-rectangle">
                @Res.Print_Excel  <i class="fa fa-file-excel-o"></i>
            </a>
        </div>


    </form>
</div>

<script src="~/ClientApp/StockMoveReport/StockMoveReport.js"></script>

<script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>